{
  "redis": {
    "host": "localhost",
    "port": 6379,
    "offsetStoreInterval": 1000,
    "db-indexes": {
      "db-offsetStore": 0,
      "db-subject": 4
    }
  },
  "client": {
    "service": {
      "transports": {
        "grpc": {
          "service": "io.restorecommerce.credential.Service",
          "timeout": 3000,
          "protos": [
            "io/restorecommerce/credential.proto"
          ],
          "protoRoot": "node_modules/@restorecommerce/protos/"
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://localhost:50053"
        ]
      },
      "endpoints": {
        "read": {}
      }
    }
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "restore-notification-srv",
      "clientId": "restore-notification-srv",
      "kafkaHost": "localhost:29092",
      "latestOffset": false,
      "sendEmail": {
        "protos": [
          "io/restorecommerce/notification.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.notification.Notification"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "queuedJob": {
        "protos": [
          "io/restorecommerce/job.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.job.ScheduledJob"
      },
      "topics": {
        "notification": {
          "topic": "io.restorecommerce.notification",
          "events": [
            "sendEmail"
          ]
        },
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "healthCheckCommand",
            "versionCommand"
          ]
        },
        "job": {
          "topic": "io.restorecommerce.jobs",
          "events": [
            "queuedJob"
          ]
        }
      }
    }
  },
  "server": {
    "mailer": {
      "debug": false,
      "tls": {
        "rejectUnauthorized": false
      },
      "queuePrefix": "restore.q",
      "host": "mail.example.com",
      "port": 25,
      "auth": {
        "user": "test@example.com",
        "pass": "testPassword"
      },
      "address": "'restorecommerce' <noreply@restorecommerce.io>"
    },
    "transports": [
      {
        "name": "notificationGRPC",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-notification-srv": "io.restorecommerce.notification.Service",
          "io-restorecommerce-notification-cis": "io.restorecommerce.commandinterface.Service",
          "io-restorecommerce-notification-reflection": "grpc.reflection.v1alpha.ServerReflection",
          "grpc-health-v1": "grpc.health.v1.Health"
        },
        "protos": [
          "io/restorecommerce/notification.proto",
          "io/restorecommerce/commandinterface.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "grpc/health/v1/health.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "0.0.0.0:50052"
      }
    ],
    "services": {
      "io-restorecommerce-notification-srv": {
        "send": {
          "transport": [
            "notificationGRPC"
          ]
        }
      },
      "io-restorecommerce-notification-cis": {
        "command": {
          "transport": [
            "notificationGRPC"
          ]
        }
      },
      "io-restorecommerce-notification-reflection": {
        "serverReflectionInfo": {
          "transport": [
            "notificationGRPC"
          ]
        }
      },
      "grpc-health-v1": {
        "check": {
          "transport": [
            "notificationGRPC"
          ]
        },
        "watch": {
          "transport": [
            "notificationGRPC"
          ]
        }
      }
    }
  },
  "serviceNames": {
    "notification": "io-restorecommerce-notification-srv",
    "cis": "io-restorecommerce-notification-cis",
    "reflection": "io-restorecommerce-notification-reflection",
    "health": "grpc-health-v1"
  }
}
